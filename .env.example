# JamboJet API Configuration

# API Base URL (Required)
JAMBOJET_BASE_URL=https://jmtest.booking.jambojet.com/jm/dotrez/

# API Subscription Key (Required)
# Get this from JamboJet support
JAMBOJET_SUBSCRIPTION_KEY=your-subscription-key-here

# Request Timeout in seconds (Optional, Default: 30)
JAMBOJET_TIMEOUT=30

# Number of retry attempts for failed requests (Optional, Default: 3)
JAMBOJET_RETRY_ATTEMPTS=3

# Environment: test, staging, production (Optional, Default: test)
JAMBOJET_ENVIRONMENT=test

# Cache Configuration
JAMBOJET_CACHE_ENABLED=true
JAMBOJET_CACHE_TTL=3600
JAMBOJET_CACHE_PREFIX=jambojet_

# Logging Configuration (Set to true for debugging)
JAMBOJET_LOG_REQUESTS=false
JAMBOJET_LOG_CHANNEL=stack